{
    "manifest_version": 3,
    "name": "Persistent Timer",
    "version": "1.0",
    "description": "A timer that persists after browser reload.",
    "permissions": ["storage"],
    "action": {
      "default_popup": "popup.html",
      "default_icon": {
        "16": "images/image.png",
        "48": "images/image.png",
        "128": "images/image.png"
      }
    },
    "icons": {
      "16": "images/image.png",
      "48": "images/image.png",
      "128": "images/image.png"
    }
  }
  